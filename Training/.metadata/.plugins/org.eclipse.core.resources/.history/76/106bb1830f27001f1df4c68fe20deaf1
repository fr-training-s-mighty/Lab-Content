package multithread;

public class synchRunner {
	
	public static void main(String[] args) {
		synch counter = new synch();
		Thread lamb = new Thread(() -> ) {
			for(int i =0;i<1000;i++) {
				counter.incremenet();
			}
		}
	}

}

class synch {
	
	private int count = 0;
	
	public void incremenet() {
		this.count++;
	}
	public int getCount() {
		return this.count;
	}

}